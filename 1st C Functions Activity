#include <stdio.h>
#include <stdlib.h>
int main()
{
   int g1, g2, g3, g4, g5;
   int x;

    printf("Enter grade number 1: ");
    scanf("%d", &g1);

    printf("Enter grade number 2: ");
    scanf("%d", &g2);

    printf("Enter grade number 3: ");
    scanf("%d", &g3);

    printf("Enter grade number 4: ");
    scanf("%d", &g4);

    printf("Enter grade number 5: ");
    scanf("%d", &g5);

    printf("Your average grade is: %d", average(addnum(g1,  g2, g3, g4, g5)));




    return 0;
}

int addnum (int g1, int g2, int g3, int g4, int g5) {
        int sum;

        sum = g1 + g2 + g3 + g4 + g5;

        return sum;
}
int average (int a){
        int ave;

        ave = a / 5;

        return ave;
